#include <iostream>
using namespace std;
int main() {
	string request = "\nEnter number of rows and columns \n";
	int rows, columns;
	cout << request;
	cin >> rows;
	cin >> columns;
	while (rows > 0 || columns > 0) {
		for (int i =0; i < rows; i++){
			for (int j = 0; j < columns; j++){
				cout << "X.";
			}
			cout << endl;
		}
		return 0;
	}
}
		

